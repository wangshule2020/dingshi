page {
    overflow:hidden; 
    position:fixed; 
    background-image:url('data:image/jpg;base64,'); 
    margin: 0px; 
    /*下面的是关键的设置100%显示*/ 
    background-size: 100% 100%; 
}
.container{
  padding:40rpx 60rpx;
  display: flex; 
  flex-direction: column;
  background-image: url(data:image/jpg;base64,);
  padding-top: 174rpx;
  padding-bottom: 250rpx;
  background-repeat: no-repeat;
  background-size: 100% 100%;
}
.container .imagesize{
  display:flex;                 
  justify-content: center;
}
.imagesize .image{
  margin-top:20px;
  width: 100px; 
  height: 100px;
}
.container .login1{
  font-weight:bold;
  font-size:60rpx;
  margin-top: 20rpx;
  margin-bottom: 40rpx;
  color:rgb(75, 78, 83);
  text-align: center;
}
.container .login{
  font-weight:bold;
  font-size:40rpx;
  margin-top: 40rpx;
  margin-bottom: 30rpx;
  color:rgb(75, 78, 83);
}
.container .textarea{
  font-size:32rpx;
  width:100%;
  height:50rpx;
  border:2rpx solid rgb(179, 171, 171);
  margin-bottom: 80rpx;
  border-radius:30rpx;
  padding:10rpx 10rpx;
  background-color: white;
}