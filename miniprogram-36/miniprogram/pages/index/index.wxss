page {
    overflow:hidden; 
    position:fixed; 
    background-image:url('data:image/jpg;base64,'); 
    margin: 0px; 
    /*下面的是关键的设置100%显示*/ 
    background-size: 100% 100%; 
}

.weidenglu{
  padding:60rpx;
}
.weidenglu .wel{
  height:600rpx;
}
.wel view{
  font-weight:bold;
  font-size:60rpx;
  margin-top: 80rpx;
  color:rgb(75, 78, 83);
  text-align: center
}


.container{
  min-height: 100%;
  padding:60rpx 40rpx;
}
.container .welcome{
  font-weight:bold;
  font-size:40rpx;
  margin-bottom: 30rpx;
  color:rgb(75, 78, 83);
}
.container .set-btn{
  padding:10rpx 15rpx;
  position:fixed;
  right:50rpx;
  top:40rpx;
  border-radius:10%;
  background:skyblue;
  display:flex;
  justify-content: center;
  align-items: center;
  font-size:40rpx;
  color:#fff;
  box-shadow:0 0 5px rgba(0,0,0,0.5);
}
.container .textarea{
  font-size:28rpx;
  width:100%;
  height:200rpx;
  border:1rpx solid #ccc;
  border-radius:30rpx;
  padding:10rpx 10rpx;
  background: white;
}
.container .form{
  color:rgb(75, 78, 83);
  font-weight:bold;
  font-size:40rpx;
  margin-top: 25rpx;
}
.container input{
   margin-bottom:40rpx;
}
.icon{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  
}
.icon .noactive{
  background-color:#fff;
  font-size:35rpx;
  padding:10rpx 20rpx;
  color:#353535;
  text-align:center;
  border-radius:40rpx;
  margin-right:20rpx;
  margin-bottom:20rpx;
}
.icon .active{
  background-color:rgba(236, 161, 196, 0.993);
  font-size:40rpx;
  padding:10rpx 20rpx;
  color:white;
  text-align:center;
  border-radius:40rpx;
  margin-right:20rpx;
  margin-bottom:20rpx;
}
.btn .submit-btn{
  margin-top:60rpx;
  width:100%;
}
.btn .cancel-btn{
  margin-top:20rpx;
  width:100%;
}
.btn .tuichu-btn{
  margin-top:20rpx;
  width:100%;
}

 .image-list-group{
  padding-top:20rpx;
  display:flex;
  justify-content: flex-start;
  flex-wrap: wrap;
}
.image-list-group .image-group{
  margin-right:5rpx;
  margin-bottom:5rpx;
  position: relative;
}
.image-group image{
  width:100%;
  height:100%;
}
.image-group .close-btn{
  width:48rpx;
  height:48rpx;
  border-radius: 50%;
  position: absolute;
  right:-30rpx;
  top:-30rpx;
  background-size:50rpx;
  background-repeat:no-repeat;
  z-index:1;
  background-color: white;
  background-image:url("data:image/png;base64,")
}
.image-list-group .add-image-btn{
  background:#f2f2f2;
  color:#a9a9a9;
  display:flex;
  justify-content: center;
  align-items: center;
}
.add-image-btn view{
  font-size:200rpx;
  margin-top:-20rpx;
}
